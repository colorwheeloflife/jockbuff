<table class="table table-bordered table-striped table-hover table-responsive table-players"
       id="otherTable"
       data-toggle="table"
       data-search="true">
  <thead>
    <tr>
      <th data-field="state" data-checkbox="true"> Trade</th>
      <th data-field="name">Player</th>
      <th data-field="pro_team">Team</th>
      <th data-field="positions">Pos</th>
      <th data-field="jbr" data-sortable="true">JBR</th>
      <th data-field="g" class="hidden">G</th>
      <th data-field="a" class="hidden">A</th>
      <th data-field="p" class="hidden">P</th>
      <th data-field="ppp" class="hidden">PPP</th>
      <th data-field="plusminus" class="hidden">+/-</th>
      <th data-field="pim" class="hidden">PIM</th>
      <th data-field="sog" class="hidden">SOG</th>
      <th data-field="fow" class="hidden">FOW</th>
      <th data-field="hit" class="hidden">HIT</th>
      <th data-field="blk" class="hidden">BLK</th>
    </tr>
  </thead>
  <tbody id="each-other-player">
    <% @trade_team.each do |player_passport| %>
    <% next if player_passport.player.positions.join("/").include?("G") %>
    <tr>
      <td></td>
      <td> <%= player_passport.player.name %> </td>
      <td> <%= player_passport.player.pro_team.upcase %></td>
      <td> <%= player_passport.player.positions.join("/") %> </td>
      <td class="jbr-stat"> <%= jbr_by_cat(1, player_passport.player.player_id, :jbr)[player_passport.player.player_id] %></td>
      <td class="hidden goals"> <%= player_passport.player.player_prediction.g %> </td>
      <td class="hidden assists"> <%= player_passport.player.player_prediction.a %> </td>
      <td class="hidden points"> <%= player_passport.player.player_prediction.p %> </td>
      <td class="hidden powerplay"> <%= player_passport.player.player_prediction.ppp %> </td>
      <td class="hidden plusminus"> <%= player_passport.player.player_prediction.plusminus %> </td>
      <td class="hidden pims"> <%= player_passport.player.player_prediction.pim %> </td>
      <td class="hidden sog"> <%= player_passport.player.player_prediction.sog %> </td>
      <td class="hidden fow"> <%= player_passport.player.player_prediction.fw %> </td>
      <td class="hidden hit"> <%= player_passport.player.player_prediction.hit %> </td>
      <td class="hidden blk"> <%= player_passport.player.player_prediction.blk %> </td>
    </tr>
    <% end %>
  </tbody>
</table>