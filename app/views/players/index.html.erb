<div class="container-fluid topfix container-space">
  <div class="col-md-6 graph" id="line_graph-top" style="width:100%; height:400px;">HI</div>
</div>
<div class="container-fluid container-space">
  <div class="row">
    <div class="col-md-3">
      <h1>Player Rankings</h1>
      <input class="form-control text-left" placeholder="Search by name">
      <button type="button" class="btn btn-block btn-default goaliebtn">Goalies</button>
    </div>
  </div>
  <table class="table table-bordered table-striped table-hover table-responsive">
  <thead>
    <tr>
      <th><%= sortable "name" %></th>
      <th><%= sortable "position" %></th>
      <th><%= sortable "g" %></th>
      <th><%= sortable "a" %></th>
      <th><%= sortable "points" %></th>
      <th><%= sortable "ppp" %></th>
      <th><%= sortable "+/-" %></th>
      <th><%= sortable "pims" %></th>
      <th><%= sortable "sog" %></th>
      <th><%= sortable "hits" %></th>
      <th><%= sortable "blks" %></th>
      <th><%= sortable "jbr" %></th>
    </tr>
  </thead>
  <tbody>
    <% @players.each do |player| %>
      <tr>
        <td> player.name </td>
        <td> player.position </td>
        <td> player.g </td>
        <td> player.a </td>
        <td> player.points </td>
        <td> player.ppp </td>
        <td> player.+/- </td>
        <td> player.pims </td>
        <td> player.sog </td>
        <td> player.hits </td>
        <td> player.blks </td>
        <td> player.jbr </td>
      </tr>
    <% end %>
  </tbody>
</table>

<table>
    <thead>
      <tr>
        <th><%= sortable "name" %></th>
        <th><%= sortable "team" %></th>
        <th><%= sortable "wins" %></th>
        <th><%= sortable "loses" %></th>
        <th><%= sortable "gaa" %></th>
        <th><%= sortable "sv %" %></th>
        <th><%= sortable "shutouts" %></th>
        <th><%= sortable "saves" %></th>
        <th><%= sortable "jbr" %></th>
      </tr>
    </thead>
    <tbody>
      <% @players.each do |player| %>
        <tr>
          <td>goalie.name</td>
          <td>goalie.team</td>
          <td>goalie.wins</td>
          <td>goalie.loses</td>
          <td>goalie.gaa</td>
          <td>goalie.sv%</td>
          <td>goalie.shutouts</td>
          <td>goalie.saves</td>
          <td>goalie.jbr</td>
        </tr>
      <% end %>
    </tbody>
  </table>

</div>
<div class="col-md-6" id="rank_column" style="width:100%; height:400px;"></div>




